@model RipMyPaperToShreds.com.Models.Shreds

<h4>Shreds</h4>
<hr />
<div class="row">
    <div class="col-md-4">
        <form asp-action="SubmitShredLayout">
            <div asp-validation-summary="ModelOnly" class="text-danger"></div>
            <div class="form-group">
                <label asp-for="Date" class="control-label"></label>
                <input asp-for="Date" class="form-control" />
                <span asp-validation-for="Date" class="text-danger"></span>
            </div>
            <div class="form-group form-check">
                <label class="form-check-label">
                    <input class="form-check-input" asp-for="Fixed" /> @Html.DisplayNameFor(model => model.Fixed)
                </label>
            </div>
            <div class="form-group">
                <label asp-for="ID" class="control-label"></label>
                <input asp-for="ID" class="form-control" />
                <span asp-validation-for="ID" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="PaperId" class="control-label"></label>
                <input asp-for="PaperId" class="form-control" />
                <span asp-validation-for="PaperId" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="Shred" class="control-label"></label>
                <input asp-for="Shred" class="form-control" />
                <span asp-validation-for="Shred" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="ShrederId" class="control-label"></label>
                <input asp-for="ShrederId" class="form-control" />
                <span asp-validation-for="ShrederId" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="ShredyId" class="control-label"></label>
                <input asp-for="ShredyId" class="form-control" />
                <span asp-validation-for="ShredyId" class="text-danger"></span>
            </div>
            <div class="form-group">
                <input type="submit" value="Create" class="btn btn-primary" />
            </div>
        </form>
    </div>
</div>

<div>
    <a asp-action="Index">Back to List</a>
</div>

