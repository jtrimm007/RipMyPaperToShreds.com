@model RipMyPaperToShreds.com.Models.ViewModels.SubmitPaperVM


        <form id="DocxInsertedForm" asp-action="_DocxInsertedForm" >
            <div asp-validation-summary="ModelOnly" class="text-danger"></div>
            <div class="form-group">
                <label asp-for="Css" class="control-label"></label>
                <input asp-for="Css" class="form-control" />
                <span asp-validation-for="Css" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="Date" class="control-label"></label>
                <input asp-for="Date" class="form-control" />
                <span asp-validation-for="Date" class="text-danger"></span>
            </div>
            <div class="form-group form-check">
                <label class="form-check-label">
                    <input class="form-check-input" asp-for="Draft" /> @Html.DisplayNameFor(model => model.Draft)
                </label>
            </div>
            <div class="form-group">
                <label asp-for="FilePath" class="control-label"></label>
                <input asp-for="FilePath" class="form-control" />
                <span asp-validation-for="FilePath" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="GuidId" class="control-label"></label>
                <input asp-for="GuidId" class="form-control" />
                <span asp-validation-for="GuidId" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="ID" class="control-label"></label>
                <input asp-for="ID" class="form-control" />
                <span asp-validation-for="ID" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="Paper" class="control-label"></label>
                <input asp-for="Paper" class="form-control" />
                <span asp-validation-for="Paper" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="PaperId" class="control-label"></label>
                <input asp-for="PaperId" class="form-control" />
                <span asp-validation-for="PaperId" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="ShrederId" class="control-label"></label>
                <input asp-for="ShrederId" class="form-control" />
                <span asp-validation-for="ShrederId" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="Title" class="control-label"></label>
                <input asp-for="Title" class="form-control" />
                <span asp-validation-for="Title" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="UniqueFileName" class="control-label"></label>
                <input asp-for="UniqueFileName" class="form-control" />
                <span asp-validation-for="UniqueFileName" class="text-danger"></span>
            </div>

        </form>

