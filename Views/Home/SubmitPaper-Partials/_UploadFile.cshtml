@model RipMyPaperToShreds.com.Models.ViewModels.UploadFormVM



<form id="UploadPaper" asp-action="UploadFile" asp-controller="Home" method="POST" enctype="multipart/form-data">
    @*<div asp-validation-summary="ModelOnly" class="text-danger"></div>*@
    <div class="form-group">
        <input asp-for="Css" class="form-control" hidden value="" />
    </div>
    <div class="form-group">
        <input asp-for="ID" class="form-control" hidden value="0" />
    </div>
    <div class="form-group">
        <input asp-for="PaperId" class="form-control" hidden value="0" />
    </div>
    <div class="form-group">
        <input asp-for="UniqueFileName" class="form-control" hidden value="Test" />
    </div>
    <div class="form-group">
        <input id="File" asp-for="File" type="file" accept=".docx" class="" />
    </div>
    <div class="form-group">
        <input asp-for="UploadDate" class="form-control" hidden value="@DateTime.UtcNow.ToString("yyyy-MM-ddTHH\\:mm\\:ss")" />
    </div>

</form>

<div class="form-group">
    <button  onclick="uploadPaper(editor)" value="Upload Paper" class="btn btn-primary" >Upload</button>
</div>



