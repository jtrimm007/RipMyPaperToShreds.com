@page
@model RipMyPaperToShreds.com.Areas.Identity.Pages.Account.Manage.MyPapersModel
@{
    ViewData["Title"] = "My Papers";
    ViewData["ActivePage"] = ManageNavPages.MyPapers;
}


<table class="table">
    <thead>
        <tr>
            <th>
                Date
            </th>
            <th>
                Draft
            </th>
            @*<th>
                Paper
            </th>*@
            @*<th>
                Shreder ID
            </th>*@
            <th>
                Title
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model.Papers)
        {
            <tr>
                <td>
                    @Html.DisplayFor(modelItem => item.Date)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.Draft)
                </td>
                @*<td>
                    @Html.DisplayFor(modelItem => item.Paper)
                </td>*@
                @*<td>
                    @Html.DisplayFor(modelItem => item.ShrederId)
                </td>*@
                <td>
                    @Html.DisplayFor(modelItem => item.Title)
                </td>
                <td>
                    <a asp-controller="Home" asp-action="EditPaper" asp-route-id="@item.ID">Edit</a> |
                    @*<a asp-action="Details" asp-route-id="@item.ID">Details</a> |
                    <a asp-action="Delete" asp-route-id="@item.ID">Delete</a>*@
                </td>
            </tr>
        }
    </tbody>
</table>

